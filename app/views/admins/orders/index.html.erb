<div class="container">
  <div class="row">
    <table class="table table-striped">
      <tr>
        <th>注文日時</th>
        <th>購入者</th>
        <th>注文個数</th>
        <th>注文ステータス</th>
      </tr>
      <% @sum = 0 %>
      <% @orders.each do |f| %>
      <tr>
        <td><%= link_to f.created_at.strftime('%Y/%m/%d %H:%M:%S'), admins_order_path(f.id) %></td>
        <td><%= f.name %></td>
        <td><%  %></td>
        <td><%= f.status_i18n %></td>
      </tr>
      <% end %>

    </table>
  </div>
</div>