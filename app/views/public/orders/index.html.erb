<div class="container">
  <div class="row">
    <table class="table table-striped">
      <tr>
        <th>注文日</th>
        <th>配送先</th>
        <th>注文商品</th>
        <th>支払金額</th>
        <th>ステータス</th>
        <th>注文詳細</th>
      </tr>
      <% @orders.each do |f| %>
      
      <tr>
        <td><%= f.updated_at.strftime('%Y/%m/%d %H:%M:%S') %></td>
        <td><%= f.postal_code %></br><%= f.address %></br><%= f.name %></td>
        <td><%=  %></td>
        <td><%= f.total_payment %></td>
        <td><%= f.status %></td>
        <td><%= link_to "表示する", public_order_path(f.id), class: "btn btn-primary" %></td>
      </tr>
      <% end %>

    </table>
  </div>
</div>